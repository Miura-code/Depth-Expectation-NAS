digraph {
	node [align=center fontname=times fontsize=20 height=0.5 penwidth=2 shape=polygon sides=5 style=filled width=0.5]
	edge [fontname=times fontsize=20]
rankdir=LR	"c_{k-2}" [fillcolor=darkgoldenrod1]
	"c_{k-1}" [fillcolor=darkgoldenrod1]
	0 [fillcolor=indianRed1]
	1 [fillcolor=indianRed1]
	2 [fillcolor=indianRed1]
	"c_{k-1}" -> 0 [label=skip_connect fillcolor=gray]
	"c_{k-2}" -> 0 [label=max_pool_3x3 fillcolor=gray]
	"c_{k-1}" -> 1 [label=skip_connect fillcolor=gray]
	"c_{k-2}" -> 1 [label=skip_connect fillcolor=gray]
	1 -> 2 [label=skip_connect fillcolor=gray]
	"c_{k-1}" -> 2 [label=skip_connect fillcolor=gray]
	"c_{k}" [fillcolor=honeydew2]
	1 -> "c_{k}" [fillcolor=gray]
	2 -> "c_{k}" [fillcolor=gray]
	fontname=times fontsize=20 label="Initial DAG" overlap=false
}
